{
	"name": "internet/intercache",
	"description": "A lightweight PHP cross-request caching library.",
	"license": "MIT",
	"keywords": [
		"cache",
		"caching",
		"psr-6",
		"psr-16"
	],

	"require": {
		"psr/cache": "1.0.1",
		"psr/simple-cache": "1.0.1",
		"ext-zlib": "*",
		"ext-json": "*",
		"fig/cache-util": "dev-master@dev"
	},
	"autoload": {
		"psr-4": {
			"Internet\\InterCache\\": "src/"
		}
	},

	"require-dev": {
		"phpunit/phpunit": ">=7 <10"
	}
}
